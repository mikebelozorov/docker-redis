{
  "data_dir": "/tmp/consul",
  "datacenter": "${CONSUL_DC}",
  "ports": {
    "dns": -1
  },
  "enable_script_checks": true,
  "service": {
    "name": "${CONSUL_SERVICE_NAME:-redis}",
    "port": 6379,
    "checks": [
      {
        "args": [
          "/check.sh"
        ],
        "interval": "10s"
      }
    ]
  }
}
